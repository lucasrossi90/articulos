    xlsx = Roo::Spreadsheet.open('./PRODS.xlsx')
	xlsx.sheet(0).each do |row|

	rowID = row
	i = row[0]
	d = row[1]
	r = row[2]
	rubronombre = row[3]
	cod = row[4]
	u = row[5]

	if Rubro.where(:id => r).exists?
	else
		Rubro.create!(
			id: r,
			nombre: rubronombre)
	end

	if cod.blank?
	cod = 'sin código'
	articulo = Articulo.create!(
    			  interno: i,
                  codigo: cod, 
                  descripcion: d, 
				  rubro_id: r,
				  ubicacion: u,
				  proveedor_id: '2')
	next
	
	elsif u.nil?
	u = 'sin ubicación'
	articulo = Articulo.create!(
		 interno: i,
         codigo: cod, 
         descripcion: d, 
		 rubro_id: r,
		 ubicacion: u,
		 proveedor_id: '2')
	next

	else
	
	articulo = Articulo.create!(
		 interno: i,
         codigo: cod, 
         descripcion: d, 
		 rubro_id: r,
		 ubicacion: u,
		 proveedor_id: '2')

	end

end

